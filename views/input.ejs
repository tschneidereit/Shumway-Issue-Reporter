<% pageTitle='Shumway Issue Reporter' %>
<% include header %>
        <form id="form" action='submit'>
        <fieldset>
          <div class='field required'>
            <label for='input-url' title="Required">URL of the page the error was encountered on</label>
            <input id="input-url" name='url' class='fill-width' type="url" required value='<%= url %>'>
          </div>
          <div class='field required'>
            <label for='input-swf' title="Required">URL of the SWF the error was encountered on</label>
            <input id="input-swf" name='swf' class='fill-width' type="url" required value='<%= swf %>'>
          </div>
          <p>Shumway version: <%= shumwayVersion %> | Firefox version: <%= firefoxVersion %></p>
          <input type='hidden' name='shumwayVersion' value='<%= shumwayVersion %>'>
          <input type='hidden' name='firefoxVersion' value='<%= firefoxVersion %>'>
          <div class='field'>
            <label for='input-email'>Your Email (optional, non-public)</label>
            <input id="input-email" name='email' class='fill-width' type="email">
          </div>
          <div class='field'>
            <label for='input-description'>Description (optional)</label>
            <textarea id="input-description" name='description' class='fill-width' rows='20'></textarea>
          </div>
          <div class="field">
            <input id='submit-form' class="button" type="submit" value="Submit">
          </div>
          </fieldset>
        </form>
        <script>
        document.getElementById('submit-form').addEventListener('click', function(event) {
          console.log(435);
          // event.preventDefault();

        });
        </script>
<% include footer %>