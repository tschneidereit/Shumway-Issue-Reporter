<% pageTitle='Shumway Issue Reporter' %>
<% include header %>
        <div>
          <h2>Reports</h2>
          <table style='text-align: left'>
            <tr>
              <th>Date</th>
              <th>Url</th>
              <th>Description</th>
            </tr>
            <% entries.forEach(function(entry) { %>
              <tr>
                <td><a href="<%= 'entry/' + entry.id %>"><%= entry.submission %></a></td>
                <td><a href="<%= entry.pageurl %>"><%= entry.pageurl.substr(0, 50) %></a></td>
                <td><%= entry.description.substr(0, 50) %></td>
                <td><%= (entry.exceptions || '').substr(0, 50) %></td>
              </tr>
            <% }); %>
          </table>
        </div>
<% include footer %>